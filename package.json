    "less": "^3.10.3"
  },
  "private": true,
  "devDependencies": {
    "axios": "^0.19",
    "bower": "^1.8.8",
    "cross-env": "^6.0",
    "grunt": "^1.0.4",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-less": "^2.0.0",
